{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
      "strict": true,
      "paths": {
          "@/*": [
              "./*"
          ]
      },
      "types": [
          "moment"  // Ajoutez cette ligne pour inclure les types de moment
      ]
  },
  "include": [
      "**/*.ts",
      "**/*.tsx",
      ".expo/types/**/*.ts",
      "expo-env.d.ts"
  ]
}
